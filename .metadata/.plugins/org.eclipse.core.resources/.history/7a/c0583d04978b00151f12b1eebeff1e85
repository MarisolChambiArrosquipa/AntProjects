<?xml version="1.0" encoding="UTF-8"?>
<project default="run" name="Examples using ant with java">
 <target name="run" depends="compile">
	 <java classname="com.myproject.test.AntExamples">
	 	<classpath path="binary" />
	 </java>
 </target>
 <target name="compile">
 <javac includeantruntime="false" srcdir="./src" destdir="binary"></javac>
 </target>
</project>